include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */ 
    int a,b;
    scanf("%d%d",&a,&b);
    int product = a * b;
    if(product > 1000)
    {
        printf("%d",a+b);
}
    else{
        printf("%d",product);
        
    }
    return 0;
}
