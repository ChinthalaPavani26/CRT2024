#include <stdio.h>

int main() {
    // Write C code here
    
    char ch ;
    scanf("c",&ch);
    if(ch == 'a' || ch == 'e' || ch == 'i' || ch == 'o' || ch == 'u'|| ch == 'A'|| ch == 'E' || ch == 'I' | ch == 'O' || ch == 'U')
    
    {
    printf("vowel");    
    }
    else
    {
        printf("Consonant");
    }
    

    return 0;
}
