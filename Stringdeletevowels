#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */  
    char str[50];
    scanf("%[^\n]s",str);
    int i,j = 0;
    for(i = 0;str[i]!='\0';i++)
    {
    if(str[i] == 'a'|| str[i] == 'A'|| str[i] == 'e'||str[i]=='E'||str[i] == 'i'||str[i] == 'I'||str[i] == 'o'||str[i]=='O'||str[i] == 'u'||str[i] == 'U')
    {
        continue;
    }
        str[j] = str[i];
        j++;

}
    str[j] = '\0';
        printf("%s",str);
    
    
    
    return 0;
}
