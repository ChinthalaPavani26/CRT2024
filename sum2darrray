#include <stdio.h>

int main() {
    // Write C code here
    int n;
    scanf("%d",&n);
    int a[n][n];
    for(int i = 0;i<n;i++)
    {
        for(int j =0;j<n;j++)
        {
            scanf("%d",&a[i][j]);
        }
    }
    int s = 0;
    for(int i = 0;i<n;i++)
    {
        for(int j =0;j<n;j++)
        {
            if(i==j)
            {
             s+=a[i][j];
            }
        }
    }
    printf("%d",s);
    

    return 0;
}
