int main()
{/* Enter your code here. Read input from STDIN. Print output to STDOUT*/

    int n;

    scanf("%d",&n);

    int r1 = 7, r2 = 5;

    for(int i = 1;i <= n; i++)

    {

      if(i%2 == 0)

        {

         printf("%d ",r2);

            r2 += 1;
        }

        else
        {
            printf("%d ",r1);
            r1 += 1;
        }
    }

    return 0;

}
